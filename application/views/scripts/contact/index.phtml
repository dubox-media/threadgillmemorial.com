<section id="contact-content" class="page-content">
	<header>
		<h1>Contact Threadgill Memorial Services</h1>
	</header>
	<section class="clear-15"></section>
	<section class="page-menu threecol">
		<section>
			<hgroup>
				<h6>--- Contact Info ---</h6>
			</hgroup>
			<article>
				<address>
					9630 SW Majorie Lane #2
				</address>
				<address>
					Beaverton, OR 97008
				</address>
				<address>
					503-526-3952
				</address>
			</article>
		</section>
	</section>
	<section id="contact-form-wrapper" class="ninecol last content-wrapper">
		<header>
			<hgroup>
				<h5>Fill out the form ( or just give us a call ) below to contact us!</h5>
			</hgroup>
		</header>
		<section id="contact-form" class="">
			<form action="/contact/submit" method="POST">
				<ul>
					<!-- First Row -->
					<li>
						<ul>
							<li class="wrapper-43"><input type="text" name="first-name" placeholder="First Name"></li>
							<li class="wrapper-53"><input type="text" name="last-name" placeholder="Last Name"></li>
						</ul>
					</li>
					<!-- Second Row -->
					<li>
						<ul>
							<li class="wrapper-43"><input type="email" name="email" placeholder="Email Address"></li>
							<li class="wrapper-53"><input type="phone" name="phone" placeholder="Phone Number"></li>
						</ul>
					</li>
					<!-- Third Row -->
					<li>
						<ul>
							<li class="wrapper-43 flush"><label for="inquiry">Type your question or comment below:</label></li>
						</ul>
					</li>
					<!-- Fourth Row -->
					<li class="flush wrapper-75"><textarea rows="10" id="inquiry" name="inquiry"></textarea></li>
					<!-- Fifth Row -->
					<li>
						<ul>
							<li class="wrapper-20 flush"><input type="submit" value="Say Hello!"></li>
						</ul>
					</li>
				</ul>
			</form>
		</section>
	</section>
	<section class="clear-15"></section>
</section>